mixin InsiderNavbar
  header.bg-body-emphasis.sticky-top(data-navbar-shadow-on-scroll)
    nav.navbar.navbar-expand-lg.container-small.px-3.px-lg-6.px-xxl-3
      a.navbar-brand.flex-1.flex-lg-grow-0(href='#top')
        +Logo
      button.navbar-toggler(type='button', data-bs-toggle='collapse', data-bs-target='#insiderNavbar', aria-controls='insiderNavbar', aria-expanded='false', aria-label='Toggle navigation')
        span.navbar-toggler-icon
      .collapse.navbar-collapse#insiderNavbar
        ul.navbar-nav.me-auto.mb-3.mb-lg-0
          - var links = [{label:'Signals', href:'#signals'},{label:'Conviction Model', href:'#method'},{label:'Product', href:'#product'},{label:'Pricing', href:'#pricing'}];
          each link, index in links
            li.nav-item.border-bottom.border-translucent.border-bottom-lg-0
              a.nav-link.lh-1.py-0.fs-9.fw-bold.py-3(href=link.href, class=index === 0 ? 'active' : '')= link.label

            li.nav-item
          .d-grid.d-lg-flex.align-items-center
            div.nav-item.d-flex.align-items-center.d-none.d-lg-block.pe-2
              +ToggleThemeBtn('themeControlToggle').px-2
            a.btn.btn-link.text-body.order-1.order-lg-0.ps-4.me-lg-2(href=`${CWD}${paths['simple-signin']}`) Sign in
            a.btn.btn-phoenix-primary.order-0(href=`${CWD}${paths['simple-signup']}`)
              span.fw-bold Sign up

