	//- Inline handler to keep Formspree submission on-page
	script.
		(() => {
			const form = document.getElementById('insiderForm');
			if (!form) return;
			const successEl = document.getElementById('insiderFormSuccess');
			const errorEl = document.getElementById('insiderFormError');
			form.addEventListener('submit', async (e) => {
				e.preventDefault();
				successEl?.classList.add('d-none');
				errorEl?.classList.add('d-none');
				if (!form.checkValidity()) {
					form.reportValidity();
					return;
				}
				try {
					const res = await fetch(form.action, {
						method: form.method,
						headers: { Accept: 'application/json' },
						body: new FormData(form),
					});
					if (res.ok) {
						form.reset();
						successEl?.classList.remove('d-none');
						form.scrollIntoView({ behavior: 'smooth', block: 'start' });
					} else {
						throw new Error('Request failed');
					}
				} catch (err) {
					errorEl?.classList.remove('d-none');
				}
			});
		})();
